{
  "extends": [
    "airbnb"
  ],
  "rules": {
    "max-len": [
      2, {
        "code": 150,
        "tabWidth": 2,
        "ignoreComments": true,
        "ignoreTrailingComments": true,
        "ignoreUrls": true,
      }
    ],
    "global-require": [
      0
    ],
    "no-underscore-dangle": [
      0
    ],
    "no-restricted-syntax": [
      0
    ],
    "new-cap": [
      0
    ],
    "no-param-reassign": [
      0
    ]
  },
}